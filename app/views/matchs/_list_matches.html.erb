<div class="card-body">
  <div class="table-responsive">
    <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
      <thead>
        <tr class="text-center">
          <th><%= t "time_title" %></th>
          <th colspan="3"><%= t "match" %></th>
          <th><%= t "stadium" %></th>
        </tr>
      </thead>
      <tbody>
        <%= render partial: "table_list_match", collection: @matches, as: :match %>
      </tbody>
    </table>
  </div>
</div>
